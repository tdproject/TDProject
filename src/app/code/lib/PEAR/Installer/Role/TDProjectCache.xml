<role version="1.0">
	<releasetypes>php</releasetypes>
	<installable>0</installable>
	<!--
	<locationconfig>tdproject_cache_backend</locationconfig>
	<locationconfig>tdproject_cache_ttl</locationconfig>
	<unusualbaseinstall />
	-->
	<phpfile/>
	<honorsbaseinstall/>
	<executable />
	<phpextension />
	<config_vars>
		<tdproject_cache_backend>
			<type>set</type>
			<default>file</default>
			<valid_set>file</valid_set>
  			<valid_set>apc</valid_set>
			<doc>Default Zend_Cache backend to use</doc>
			<prompt>Default Zend_Cache backend</prompt>
			<group>Advanced</group>
		</tdproject_cache_backend>
		<tdproject_cache_ttl>
			<type>string</type>
			<default>0</default>
			<doc>Default TTL for backend cache</doc>
			<prompt>TTL Zend_Cache backend</prompt>
			<group>Advanced</group>
		</tdproject_cache_ttl>
	</config_vars>
</role>